{
  "name": "goodguydaniel.com",
  "version": "1.0.0",
  "description": "Blog and Home of Daniel Caldas",
  "starter-name": "gatsby-starter-minimal-blog",
  "scripts": {
    "build": "npm run clean && gatsby build",
    "dev": "gatsby develop",
    "lint": "eslint --config=.eslintrc.js \"src/**/*.{js,jsx,ts,tsx}\" \"content/**/*.{js,jsx,ts,tsx}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-minimal-blog": "^2.3.5",
    "disqus-react": "^1.0.8",
    "gatsby": "^2.20.36",
    "gatsby-plugin-google-analytics": "^2.1.4",
    "gatsby-plugin-manifest": "^2.2.3",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-sitemap": "^2.2.19",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-gif-player": "^0.4.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.0.1",
    "@typescript-eslint/parser": "^3.0.1",
    "cross-env": "^5.2.0",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielcaldas/goodguydaniel.com.git"
  },
  "keywords": [
    "blog"
  ],
  "author": "Daniel Caldas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielcaldas/goodguydaniel.com/issues"
  },
  "homepage": "https://github.com/danielcaldas/goodguydaniel.com#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{css,scss,less,json,json5,graphql,md,mdx,yaml,yml,html}": [
      "prettier --write",
      "git add"
    ]
  }
}
